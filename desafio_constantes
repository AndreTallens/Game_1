// Escreva um programa que declara e inicializa as seguintes constantes:

// Uma constante chamada MAX_QUANTITY com o valor 50.
// Uma constante chamada COMPANY_NAME com o valor "Coddy Inc.".

using System;

public class Program {
    public static void Main(string[] args) {
        // Declare constants here
        const int MAX_QUANTITY = 50;
        const string COMPANY_NAME = "Coddy Inc";
        
        // Don't change below
        Console.WriteLine("Max Quantity: " + MAX_QUANTITY);
        Console.WriteLine("Company Name: " + COMPANY_NAME);
    }
}